{
  "name": "lbaws",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "nodemon src/index.ts",
    "debug": "nodemon --exec \"node --inspect=0.0.0.0:9229 --require ts-node/register --enable-source-maps src/index.ts\"",
    "prisma-pull": "prisma db pull && prisma generate",
    "routes-generator": "ts-node src/scripts/routes-generator.ts src/routes/db/auto",
    "types-generator": "ts-node src/scripts/types-generator.ts ../lba/src/app/shared/types/db/auto",
    "refresh-persistent": "npm run prisma-pull && npm run routes-generator && npm run types-generator"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.4.1",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.7",
    "nodemon": "^3.1.9",
    "prisma": "^6.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
